                        "value": {
                          "fragmentLink": {},
                          "text": {
                            "value_i18n": {
                              "fr_FR": "supplémentaires&nbsp;par semaine"
                            }
                          }
                        }
                      },
                      {
                        "id": "chiffre3",
                        "value": {
                          "fragmentLink": {},
                          "text": {
                            "value_i18n": {
                              "fr_FR": "800 trains"
                            }
                          }
                        }
