                      {
                        "id": "chiffre4",
                        "value": {
                          "fragmentLink": {},
                          "text": {
                            "value_i18n": {
                              "fr_FR": "20 000 voyageurs"
                            }
                          }
                        }
                      },
                      {
                        "id": "precision4",
                        "value": {
