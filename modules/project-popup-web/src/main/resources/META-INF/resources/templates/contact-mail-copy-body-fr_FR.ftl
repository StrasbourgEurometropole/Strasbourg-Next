<body leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0">
<center>

    <table align="center" border="0" cellpadding="0" cellspacing="0" height="100%" width="100%" class="w600"
           style="background: #e6e6e6;"
    ">
    <tbody>

    <!-- HEADER -->
    <tr>
        <td align="center" valign="top">
            <table class="w600" width="600" border="0" cellpadding="0" cellspacing="0" style="background: #FFFFFF;">
                <tbody class="block">
                <tr class="block">
                    <td width="600" height="143" align="center" class="height-auto">
                        <a href="#" target="_blank" style="height: 100%;display: block;">
                            <img src="${headerImage}" width="600" height="143"
                                 alt="/" class="responsive-100"/>
                        </a>
                    </td>
                </tr>
                </tbody>
            </table>
        </td>
    </tr>


    <!-- CONTENT -->
    <tr>
        <td valign="top" align="center">
            <table width="600" border="0" cellpadding="0" cellspacing="0" class="responsive-marge"
                   style="background: #FFFFFF;">
                <tbody>
                <tr>
                    <td width="500" class="w600" valign="top" align="center"
                        style="padding-top:26px;padding-bottom: 35px; display: block; margin: auto;">
                        <span style="font-family: Arial, sans-serif;font-size: 24px;line-height: 32px;color:#0c0c0c;font-weight: bold;display: block;text-align: center;">${initiativeTitle}</span>
                        <p style="font-family: Arial, sans-serif;font-size: 14px;line-height:23px;color:#a0a0a0;margin-bottom: 25px;">
                            ${initiativeHelpMessage}</p>
                    </td>
                </tr>
                </tbody>
            </table>
        </td>
    </tr>


    <!-- FOOTER - COPYRIGHT -->
    <tr>
        <td valign="top" align="center" style="background: #E6E6E6;">
            <table width="600" class="w600" border="0" cellpadding="0" cellspacing="0">
                <tbody>
                <tr>
                    <td class="responsive-marge" valign="top" align="left"
                        style="padding-top: 26px; padding-bottom: 15px; font-size: 11px;line-height: 17px;font-family: Arial, sans-serif; color: #666666;">
                    </td>
                </tr>
                </tbody>
            </table>
        </td>
    </tr>

    </tbody>
</table>


</center>
        </body>