<!-- QUITTER LE FORMULAIRE -->
<!-- HTML pour la modal de quitter un formulaire-->
<div class="pro-modal pro-bloc-pcs-form fade" id="modalQuit" tabindex="-1" role="dialog" aria-labelledby="modalQuit">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="pro-modal-top">
                <h3><liferay-ui:message key='quitModal'/></h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"><span class="icon-multiply"></span></span></button>
            </div>
            <div class="pro-wrapper">
                <h4><liferay-ui:message key='petition-are-you-sure'/></h4>
                    <input id="quitButton" type="submit" class="pro-btn" value="Quitter" />
            </div>
        </div>
    </div>
</div>