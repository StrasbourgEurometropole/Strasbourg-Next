<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder  package-path="eu.strasbourg.service.council">
	<namespace>council</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->

	<entity local-service="true" remote-service="true" cache-enabled="true" name="Official" uuid="true">

		<!-- PK fields -->
		<column name="officialId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Workflow fields -->
		<column name="status" type="int" />
		<column name="statusByUserId" type="long" />
		<column name="statusByUserName" type="String" />
		<column name="statusDate" type="Date" />

		<!-- Business fields -->
		<column name="email" type="String" />
		<column name="firstname" type="String" />
		<column name="lastname" type="String" />
		<column name="isMunicipal" type="boolean" />
		<column name="isEurometropolitan" type="boolean" />
		<column name="isActive" type="boolean" />

		<!-- Order -->
		<order by="asc">
			<order-column name="lastname" />
		</order>

		<!-- Finder methods -->
		<finder name="Email" return-type="Official">
			<finder-column name="email" />
		</finder>
		<finder name="GroupIdAndIsActive" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="isActive" />
		</finder>
		<finder name="GroupIdAndIsActiveAndIsMunicipal" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="isActive" />
			<finder-column name="isMunicipal" />
		</finder>
		<finder name="GroupIdAndIsActiveAndIsEurometropolitan" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="isActive" />
			<finder-column name="isEurometropolitan" />
		</finder>

		<!-- References -->
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetTag" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetLink" package-path="com.liferay.portlet.asset" />

	</entity>

	<entity local-service="true" remote-service="false" cache-enabled="true" name="CouncilSession" uuid="true">

		<!-- PK fields -->
		<column name="councilSessionId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Workflow fields -->
		<column name="status" type="int" />
		<column name="statusByUserId" type="long" />
		<column name="statusByUserName" type="String" />
		<column name="statusDate" type="Date" />

		<!-- Business fields -->
		<column name="title" type="String" />
		<column name="date" type="Date" />
		<column name="type" type="String" />

		<!-- Links with other entities -->
		<column name="officialLeaderId" type="long" />

		<!-- Order -->
		<order by="asc">
			<order-column name="title" />
		</order>

		<!-- Finder methods -->
		<finder name="Date" return-type="Collection">
			<finder-column name="date" />
		</finder>

		<!-- References -->
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetTag" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetLink" package-path="com.liferay.portlet.asset" />

	</entity>

	<entity local-service="true" remote-service="true" cache-enabled="true" name="Deliberation" uuid="true">

		<!-- PK fields -->
		<column name="deliberationId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Workflow fields -->
		<column name="status" type="int" />
		<column name="statusByUserId" type="long" />
		<column name="statusByUserName" type="String" />
		<column name="statusDate" type="Date" />

		<!-- Business fields -->
		<column name="title" type="String" />
		<column name="order" type="int" />
		<column name="stage" type="String" />
		<column name="countOfficialsVoting" type="int" />
		<column name="countOfficialsActive" type="int" />

		<!-- Links with other entities -->
		<column name="councilSessionId" type="long" />

		<!-- Order -->
		<order by="asc">
			<order-column name="title" />
		</order>

		<!-- Finder methods -->
		<finder name="CouncilSessionId" return-type="Collection">
			<finder-column name="councilSessionId" />
		</finder>

		<!-- References -->
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetTag" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetLink" package-path="com.liferay.portlet.asset" />

	</entity>

	<entity local-service="true" remote-service="false" cache-enabled="true" name="Procuration" uuid="true">

		<!-- PK fields -->
		<column name="procurationId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Workflow fields -->
		<column name="status" type="int" />
		<column name="statusByUserId" type="long" />
		<column name="statusByUserName" type="String" />
		<column name="statusDate" type="Date" />

		<!-- Links with other entities -->
		<column name="officialVotersId" type="long" />
		<column name="officialUnavailableId" type="long" />
		<column name="councilSessionId" type="long" />
		<column name="isAbsent" type="boolean" />

		<!-- Order -->
		<order by="desc">
			<order-column name="procurationId" />
		</order>

		<!-- Finder methods -->
		<finder name="CouncilSessionId" return-type="Collection">
			<finder-column name="councilSessionId" />
		</finder>
		<finder name="CouncilSessionIdAndOfficialVotersId" return-type="Collection">
			<finder-column name="councilSessionId" />
			<finder-column name="officialVotersId" />
		</finder>
		<finder name="CouncilSessionIdAndOfficialVotersAndUnavailableIds" return-type="Procuration">
			<finder-column name="councilSessionId" />
			<finder-column name="officialVotersId" />
			<finder-column name="officialUnavailableId" />
		</finder>

	</entity>

	<entity local-service="true" remote-service="false" cache-enabled="true" name="Vote" uuid="true">

		<!-- PK fields -->
		<column name="voteId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="createDate" type="Date" />
		<column name="result" type="String" />

		<!-- Links with other entities -->
		<column name="officialId" type="long" />
		<column name="deliberationId" type="long" />
		<column name="officialProcurationId" type="long" />

		<!-- Order -->
		<order by="desc">
			<order-column name="voteId" />
		</order>

		<!-- Finder methods -->
		<finder name="DeliberationId" return-type="Collection">
			<finder-column name="deliberationId" />
		</finder>
		<finder name="DeliberationIdAndOfficialId" return-type="Vote">
			<finder-column name="deliberationId" />
			<finder-column name="officialId" />
		</finder>

	</entity>

</service-builder>